readgroup = subset1_100X
technology = solexa
template_size = 150 250 autorefine
segment_placement = ---> <---
data = /scratch/workspace2/data/sub1.100x.fwd.paired.9gb-10249472_81PWHABXX_s_6.trim28.cor.quake.fastq  /scratch/workspace2/data/sub1.100x.rev.paired.9gb-10249472_81PWHABXX_s_6.trim28.cor.quake.fastq
